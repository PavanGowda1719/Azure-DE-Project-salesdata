{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-southindia-projects"
		},
		"azuresql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azuresql'"
		},
		"lk_adls_salesdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lk_adls_salesdata'"
		},
		"lk_sql_git_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lk_sql_git'"
		},
		"Githuhconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "south-india-sales-project.database.windows.net"
		},
		"azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesdata"
		},
		"azuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Pavan"
		},
		"lk_adls_salesdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssouthindiastorage.dfs.core.windows.net/"
		},
		"lk_sql_git_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "south-india-sales-project.database.windows.net"
		},
		"lk_sql_git_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesdata"
		},
		"lk_sql_git_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Pavan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-southindia-projects_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azuresql_password": {
						"value": "[parameters('azuresql_password')]"
					},
					"lk_adls_salesdata_accountKey": {
						"value": "[parameters('lk_adls_salesdata_accountKey')]"
					},
					"lk_sql_git_password": {
						"value": "[parameters('lk_sql_git_password')]"
					},
					"Githuhconnection_properties_typeProperties_url": {
						"value": "[parameters('Githuhconnection_properties_typeProperties_url')]"
					},
					"azuresql_properties_typeProperties_server": {
						"value": "[parameters('azuresql_properties_typeProperties_server')]"
					},
					"azuresql_properties_typeProperties_database": {
						"value": "[parameters('azuresql_properties_typeProperties_database')]"
					},
					"azuresql_properties_typeProperties_userName": {
						"value": "[parameters('azuresql_properties_typeProperties_userName')]"
					},
					"lk_adls_salesdata_properties_typeProperties_url": {
						"value": "[parameters('lk_adls_salesdata_properties_typeProperties_url')]"
					},
					"lk_sql_git_properties_typeProperties_server": {
						"value": "[parameters('lk_sql_git_properties_typeProperties_server')]"
					},
					"lk_sql_git_properties_typeProperties_database": {
						"value": "[parameters('lk_sql_git_properties_typeProperties_database')]"
					},
					"lk_sql_git_properties_typeProperties_userName": {
						"value": "[parameters('lk_sql_git_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}